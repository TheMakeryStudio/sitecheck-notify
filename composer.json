{
    "name": "thenavigators/sitecheck-notify",
    "description": "Check a list of urls in a Laravel project and email recipients if down.",
    "keywords": ["laravel", "sitecheck", "notifier"],
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "TheNavigators\\SiteCheckNotify\\": "src/"
        }
    },
    "require": {
        "illuminate/support": "5.2.*",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "~3.1"
    },    
    "extra" : {
        "laravel": {
            "providers": [
                "TheNavigators\\SiteCheckNotify\\SiteCheckProvider"
            ]
        }
    }    
}
